.doc-footer{ color:#999;font-size:14px;text-align:center}
.doc-footer_fixed-bottom{ position:fixed;bottom:.52em;left:0;right:0}
.doc-footer__links{ font-size:0}
.doc-footer__link{ display:inline-block;vertical-align:top;margin:0 .62em;position:relative;font-size:14px;color:#586c94}
.doc-footer__link:before{ content:" ";position:absolute;left:0;top:0;width:1px;bottom:0;border-left:1rpx solid #c7c7c7;color:#c7c7c7;left:-.65em;top:.36em;bottom:.36em}
.doc-footer__link:first-child:before{ display:none}
.doc-footer__text{ padding:0 .34em;font-size:12px}